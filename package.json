{
  "name": "@ilyasemenov/eslint-config",
  "type": "module",
  "version": "1.4.3",
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "description": "Opinionated ESLint config on top of @antfu/eslint-config",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/eslint-config"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "version": "10",
      "onFail": "warn"
    }
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "lint": "eslint --fix .",
    "test": "vitest run"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "eslint-plugin-perfectionist": "^4.11.0",
    "eslint-plugin-vue-pug": "^1.0.0-alpha.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.2",
    "@ilyasemenov/eslint-config": "workspace:*",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "tsconfig-vite-node": "^1.1.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
