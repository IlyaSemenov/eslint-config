{
  "name": "@ilyasemenov/eslint-config",
  "type": "module",
  "version": "1.5.0",
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "description": "Opinionated ESLint config on top of @antfu/eslint-config",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/eslint-config"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "version": "10",
      "onFail": "warn"
    }
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "lint": "eslint --fix .",
    "test": "vitest run"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-vue": "10.6.2",
    "eslint-plugin-vue-pug": "1.0.0-alpha.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@ilyasemenov/eslint-config": "workspace:*",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsconfig-vite-node": "^1.2.0",
    "tsdown": "0.20.0-beta.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
